      "source": [
        "#What we want to do right here is to re-train the entire dataset (80%) and use it to predict our dataset\n",
        "\n",
        "X_train['DateOfBirth'] = pd.to_datetime(X_train.DateOfBirth.str.replace('-',''))\n",
        "X_train['Year']= X_train['DateOfBirth'].dt.year\n",
        "X_train['Month'] = X_train['DateOfBirth'].dt.month\n",
        "X_train['Day'] = X_train['DateOfBirth'].dt.day\n",
        "\n",
        "X_test['DateOfBirth'] = pd.to_datetime(X_test.DateOfBirth.str.replace('-',''))\n",
        "X_test['Year']= X_test['DateOfBirth'].dt.year\n",
        "X_test['Month'] = X_test['DateOfBirth'].dt.month\n",
        "X_test['Day'] = X_test['DateOfBirth'].dt.day\n",
        "\n",
        "scaler = preprocessing.MinMaxScaler().fit(X_train[['Amount']])\n",
        "X_train['Amount_MinMax'] = scaler.transform(X_train[['Amount']]) \n",
        "\n",
        "scaler = preprocessing.MinMaxScaler().fit(X_test[['Amount']])\n",
        "X_test['Amount_MinMax'] = scaler.transform(X_test[['Amount']])  \n",
        "\n",
        "X_train['age']=2021 - X_train['Year']\n",
        "X_test['age']=2021 - X_test['Year']\n",
        "\n",
        "X_train['Default&Age']=X_train['PreviousDefault']* X_train['age']\n",
        "X_test['Default&Age']=X_test['PreviousDefault']* X_test['age']\n",
        "\n",
        "X_train['Default&Amount']=X_train['PreviousDefault']* X_train['Amount']\n",
        "X_test['Default&Amount']=X_test['PreviousDefault']* X_test['Amount']"
      ],
      "execution_count": null,
      "outputs": []
    },
