        "X_training['age']=2021 - X_training['Year'] #I feel like birthday isn't a good represenation for prediciting. Actual age could help the model to understand the relationship between age and bad credit\n",
        "X_testing['age']=2021 - X_testing['Year']\n",
        "\n",
        "X_training['Default&Age']=X_training['PreviousDefault']* X_training['age'] # I want to see the interaction between previous default and age, and hoping to see if this new feature would improve the model.\n",
        "X_testing['Default&Age']=X_testing['PreviousDefault']* X_testing['age']\n",
        "\n",
        "X_training['Default&Amount']=X_training['PreviousDefault']* X_training['Amount'] # I want to see the interaction between default and amount. Hoping to see if there are some sort of relationship between previous default and amount\n",
