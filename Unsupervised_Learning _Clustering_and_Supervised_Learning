        "scaler = StandardScaler()  #Standard scaler to standadize the data. It helps with scaling the features change, but keep the shape the way it is.\n",
        "Hierarchial_data = scaler.fit_transform(Hierarchial_data)"
      ],
      "execution_count": null,
